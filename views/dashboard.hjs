<html>
  <head>
    <title></title>
    <link rel="stylesheet" href="/styles/bootstrap.min.css" />
    <link rel="stylesheet" href="/styles/style.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
    />
    <script src="/js/jquery-2.2.0.min.js" type="text/javascript"></script>
    <script src="/js/bootstrap.min.js" type="text/javascript"></script>
    <style>
      .welcome-message { font-size: 20px; font-weight: bold; color: #333;
      margin-top: 20px; } #user-name { color: #007bff; }
    </style>
  </head>
  <body>
    <script>
      $(document).ready(function() { const sessionToken =
      document.cookie.includes("session_token"); if (!sessionToken) {
      location.href = "/"; } });
    </script>
    <div class="container" style="margin-top: 20px;">
      <div>
        <nav class="navbar navbar-inverse">
          <div class="container-fluid">
            <div class="navbar-header">
              <a class="navbar-brand" href="/dashboard">Dashboard</a>
            </div>
            <ul class="nav navbar-nav">
              <li class="active">
                <a href="/dashboard">
                  <i class="fas fa-home"></i>
                  Home
                </a>
              </li>
              <li>
                <a href="/stations_example">
                  <i class="fas fa-subway"></i>
                  Station
                </a>
              </li>
              <li>
                <a href="/users">
                  <i class="fas fa-users"></i>
                  Users
                </a>
              </li>
              <li>
                <a href="/users/add">
                  <i class="fas fa-user-plus"></i>
                  Add Users
                </a>
              </li>
              <li class="dropdown">
                <a
                  href="#"
                  class="dropdown-toggle"
                  data-toggle="dropdown"
                  role="button"
                  aria-haspopup="true"
                  aria-expanded="false"
                >
                  <i class="fas fa-shopping-cart"></i>
                  Shop
                  <span class="caret"></span>
                </a>
                <ul class="dropdown-menu">
                  <li>
                    <a href="/tickets">
                      <i class="fas fa-ticket-alt"></i>
                      Tickets
                    </a>
                  </li>
                  <!-- Add other shop-related options here -->
                </ul>
              </li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
              <li class="dropdown">
                <a
                  href="#"
                  class="dropdown-toggle"
                  data-toggle="dropdown"
                  role="button"
                  aria-haspopup="true"
                  aria-expanded="false"
                >
                  <i class="fas fa-user-cog"></i>
                  User Options
                  <span class="caret"></span>
                </a>
                <ul class="dropdown-menu">
                  <li>
                    <a href="/resetPassword">
                      <i class="fas fa-lock"></i>
                      Reset Password
                    </a>
                  </li>
                  <li>
                    <a href="/requests/refund">
                      <i class="fas fa-undo"></i>
                      Refund Ticket
                    </a>
                  </li>
                  <!-- Add other user-related options here -->
                </ul>
              </li>
              <li>
                <a href="/"><i class="fas fa-sign-out-alt"></i> Logout</a>
              </li>
            </ul>
          </div>
        </nav>
      </div>
      <p class="welcome-message">Welcome
        <span id="user-name">{{firstName}}</span>, you are logged in!</p>
    </div>
  </body>
</html>